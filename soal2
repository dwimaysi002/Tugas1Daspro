program nama_bulan;

uses crt;

begin
    clrscr;
    writeln('Daftar Nama-nama Bulan:');
    writeln('Januari');
    writeln('Februari');
    writeln('Maret');
    writeln('April');
    writeln('Mei');
    writeln('Juni');
    writeln('Juli');
    writeln('Agustus');
    writeln('September');
    writeln('Oktober');
    writeln('November');
    writeln('Desember');
    readln;
end.
